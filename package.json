{
  "name": "react-antd-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\"",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "craco start FAST_REFRESH=true",
    "test": "craco test"
  },
  "lint-staged": {
    "**/*.less": "stylelint",
    "**/*.{js,jsx,ts,tsx}": "yarn lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "antd": "^4.21.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "i18next": "^21.8.10",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "react": "^18.2.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.2",
    "react-icons": "^4.4.0",
    "react-player": "^2.10.1",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@umijs/fabric": "^2.12.0",
    "babel-eslint": "^10.1.0",
    "circular-dependency-plugin": "^5.2.2",
    "craco-css-modules": "^1.0.1",
    "craco-less": "^2.0.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "mockjs": "^1.1.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.0",
    "react-scripts": "5.0.1",
    "stylelint": "^13.0.0",
    "swagger-ui-react": "^4.12.0",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.0",
    "webpackbar": "^5.0.2"
  }
}
