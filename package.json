    "react": "^18.2.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.2",
    "react-icons": "^4.4.0",
    "react-player": "^2.10.1",
    "react-router-dom": "^6.3.0",
    "use-react-router-breadcrumbs": "^3.2.1"
  },
